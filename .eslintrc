{
  // Extend existing configuration
  // from ESlint and eslint-plugin-react defaults.
  "extends": [
    "eslint:recommended"
  ],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  // Enable custom plugin known as eslint-plugin-react
  "plugins": [],
  "globals": {
    // Or /* global $ */ at the top of js file.
    "$": true
  },
  "rules": {
    "eol-last": 2,
    "no-undef": 2,
    "no-trailing-spaces": 2,
    "no-debugger": 0,
    "no-console": 0,
    "new-cap": 0,
    "strict": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "quotes": [2, "single"]
  }
}
